@page "/problemset"
@inject HttpClient http
@inject NavigationManager navigationManager

<div class="row" style="margin-top: 20px">
    <div class="col-md-9">
        @if(Dictants != null)
        { 
        @foreach (var model in Dictants)
        {
            <Dictant.Web.Components.Problemset.ProblemItemComponent Source="@model"/>
        }
        }
    </div>
    <div class="col-md-3">
        <SearchComponent></SearchComponent>
    </div>
</div>
